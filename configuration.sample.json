{
  "general": {
    "implementation": 2,
    "verbose": 1
  },
  "logger": {
    "log_format": "(%(asctime)s) %(name)s [%(levelname)s]: %(message)s",
    "log_file": "seq2seq.log",
    "log_level": "info"
  },
  "export": {
    "model_save_template": "{name}_model.dat"
  },
  "data": {
    "sampling_step": 1,
    "normalization": "mvn",
    "user_count": 40,
    "input_dimension": 14,
    "genuine_sample_count": 20,
    "forged_sample_count": 20,
    "genuine_path_template": "data_set/U{user}S{sample}.TXT",
    "forged_path_template": "data_set/U{user}S{sample}.TXT",
    "feature_count": 4
  },
  "autoencoder": {
    "mask_value": 0.0,
    "batch_size": 32,
    "loss": "mean_squared_error",
    "optimizer": {
      "name": "Adam",
      "args": {
      }
    },
    "metrics": [
      "accuracy"
    ],
    "encoded_length": {
      "start": 500,
      "finish": 500,
      "step": 100
    },
    "train_epochs": 20,
    "cell_types": [
      "LSTM"
    ]
  },
  "classifier": {
    "batch_size": 32,
    "train_epochs": 1000,
    "activation": "sigmoid",
    "loss": "mean_squared_error",
    "optimizer": {
      "name": "sgd",
      "args": {
      }
    },
    "metrics": [
      "accuracy"
    ]
  }
}
