{
  "general": {
    "implementation": 2,
    "verbose": 1
  },
  "logger": {
    "log_format": "(%(asctime)s) %(name)s [%(levelname)s]: %(message)s",
    "log_file": "./logs/{ct}-{earc}-{darc}-{epc}/seq2seq.log",
    "log_level": "info"
  },
  "tensorboard": {
    "log_dir": "./logs/{ct}-{earc}-{darc}-{epc}/tensorboard/U{usr_num}"
  },
  "export": {
    "model_save_template": "U{usr_num}.dat",
    "csv_fieldnames": [
      "Writer No",
      "Gen-AVG-len",
      "Frg-AVG-len",
      "Precision",
      "Recall",
      "F1-Score"
    ]
  },
  "data": {
    "sampling_step": 1,
    "normalization": "ms",
    "rolling_window_size": 1,
    "rolling_window_step": 1,
    "user_count": 40,
    "input_dimension": 14,
    "genuine_sample_count": 20,
    "forged_sample_count": 20,
    "genuine_path_template": "./data_set/U{user}S{sample}.TXT",
    "forged_path_template": "./data_set/U{user}S{sample}.TXT",
    "feature_count": 4
  },
  "autoencoder": {
    "mask_value": 0.0,
    "batch_size": 32,
    "loss": "binary_crossentropy",
    "optimizer": {
      "name": "adam",
      "args": {
      }
    },
    "metrics": [
      "accuracy"
    ],
    "encoder_architecture": [
      10,
      10
    ],
    "decoder_architecture": [
      10,
      14
    ],
    "train_epochs": 50,
    "cell_type": "GRU"
  }
}
