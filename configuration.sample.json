{
  "general": {
    "implementation": 2,
    "verbose": 1
  },
  "logger": {
    "log_format": "(%(asctime)s) %(name)s [%(levelname)s]: %(message)s",
    "log_file": "seq2seq.log",
    "log_level": "info"
  },
  "export": {
    "model_save_template": "{name}_model.dat"
  },
  "data": {
    "user_count": 11,
    "input_dimension": 2,
    "genuine_sample_count": 42,
    "forged_sample_count": 36,
    "forger_count": 4,
    "genuine_path_template": "data_set/Genuine/{user}/{sample}_{user}.HWR",
    "forged_path_template": "data_set/Forged/{user}/{sample}_{forger}_{user}.HWR"
  },
  "autoencoder": {
    "with_forged": false,
    "mask_value": 0.0001,
    "batch_size": 13,
    "loss": "mean_squared_error",
    "optimizer": {
      "name": "Adam",
      "args": {
      }
    },
    "metrics": [
      "accuracy"
    ],
    "encoded_length": {
      "start": 500,
      "finish": 500,
      "step": 100
    },
    "train_epochs": 5,
    "cell_types": [
      "LSTM"
    ]
  },
  "classifier": {
    "batch_size": 32,
    "train_epochs": 1000,
    "activation": "sigmoid",
    "loss": "mean_squared_error",
    "optimizer": {
      "name": "sgd",
      "args": {
      }
    },
    "metrics": [
      "accuracy"
    ]
  }
}
