{
  "general": {
    "verbose": 1,
    "output_directory_template": "./models/{bd}{ct}-{earc}-{darc}-{epc}"
  },
  "logger": {
    "log_format": "(%(asctime)s) %(name)s [%(levelname)s]: %(message)s",
    "log_file": "./logs/{bd}{ct}-{earc}-{darc}-{epc}/seq2seq.log",
    "log_level": "info"
  },
  "tensorboard": {
    "log_dir_template": "./logs/{bd}{ct}-{earc}-{darc}-{epc}/tensorboard/U{usr_num}",
    "histogram_freq": 1,
    "write_images": true
  },
  "export": {
    "model_save_template": "U{usr_num}.dat",
    "csv_fieldnames": [
      "Writer No",
      "Gen-AVG-len",
      "Frg-AVG-len",
      "Precision",
      "Recall",
      "F1-Score"
    ]
  },
  "data": {
    "sampling_step": 1,
    "normalization": "ms",
    "rolling_window_size": 1,
    "rolling_window_step": 1,
    "user_count": 40,
    "input_dimension": 9,
    "genuine_sample_count": 20,
    "forged_sample_count": 20,
    "genuine_path_template": "./data_set/U{user}S{sample}.TXT",
    "forged_path_template": "./data_set/U{user}S{sample}.TXT",
    "feature_count": 4
  },
  "rnn": {
    "autoencoder": {
      "mask_value": 0.0,
      "batch_size": 16,
      "layers_config": {
        "return_sequences": true,
        "go_backwards": false,
        "stateful": false,
        "unroll": false,
        "implementation": 2,
        "activation": "relu",
        "recurrent_activation": "hard_sigmoid",
        "use_bias": true,
        "kernel_initializer": "glorot_uniform",
        "recurrent_initializer": "orthogonal",
        "bias_initializer": "zeros",
        "kernel_regularizer": null,
        "recurrent_regularizer": null,
        "bias_regularizer": null,
        "activity_regularizer": null,
        "kernel_constraint": null,
        "recurrent_constraint": null,
        "bias_constraint": null,
        "dropout": 0.0,
        "recurrent_dropout": 0.0
      },
      "compile_config": {
        "optimizer": {
          "name": "adam",
          "args": {
          }
        },
        "loss": "mean_squared_error",
        "metrics": [
          "accuracy"
        ]
      },
      "encoder_architecture": [
        9,
        150
      ],
      "decoder_architecture": [
        9
      ],
      "bidirectional": true,
      "bidirectional_merge_mode": "ave",
      "train_epochs": 50,
      "cell_type": "GRU"
    }
  }
}
